<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<converter>
		<converter-id>loggerNameConverter</converter-id>
		<converter-class>jamon.ui.converter.LoggerNameConverter</converter-class>
	</converter>
	<converter>
		<converter-for-class>java.util.logging.Level</converter-for-class>
		<converter-class>jamon.ui.converter.LevelConverter</converter-class>
	</converter>

</faces-config>
